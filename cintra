                {name: "!–∞—Å—Ç–∞–ª–∞–≤–∏—Å—Ç–∞", description: "", cost: 300},
                {name: "!–∫–∏–Ω–∞ –Ω–µ –±—É–¥–µ—Ç", description: "–î–∂–µ–Ω—Ç–µ–ª—å–º–µ–Ω—ã —É–¥–∞—á–∏", cost: 300},
                {name: "!—É–ª—ã–±–∞–µ–º—Å—è –∏ –º–∞—à–µ–º", description: "–ú–∞–¥–∞–≥–∞—Å–∫–∞—Ä", cost: 300},
                {name: "!–≤—Å–µ–º –ø–æ–∫–∞", description: "–º–µ–º", cost: 300},
                {name: "!—Å–∫–æ—Ä–æ –≤—Å—Ç—Ä–µ—Ç–∏–º—Å—è", description: "", cost: 300}
            ]
        };

        // DOM Elements
        const categoriesTabs = document.getElementById('categories-tabs');
        const commandsContainer = document.getElementById('commands-container');
        const notification = document.getElementById('notification');
        const notificationText = document.getElementById('notification-text');
        const loading = document.getElementById('loading');
        const searchBox = document.getElementById('search-box');
        const noResults = document.getElementById('no-results');
        const resultsInfo = document.getElementById('results-info');
        const resultsCount = document.getElementById('results-count');
        const clearSearch = document.getElementById('clear-search');
        const guideSteps = document.getElementById('guide-steps');
        const guideToggle = document.getElementById('guide-toggle');
        
        let activeCategory = "–í—Å–µ";
        let isInitialized = false;
        let searchQuery = "";
        let allCommands = [];
        let isGuideExpanded = false;

        // Prepare all commands array
        function prepareAllCommands() {
            allCommands = [];
            Object.entries(commandsByCategory).forEach(([category, commands]) => {
                commands.forEach(command => {
                    allCommands.push({
                        ...command,
                        category: category
                    });
                });
            });
            
            // Sort all commands alphabetically
            allCommands.sort((a, b) => a.name.localeCompare(b.name));
        }

        // Initialize everything
        function initialize() {
            if (isInitialized) return;
            
            showLoading();
            setTimeout(() => {
                prepareAllCommands();
                initCategories();
                initGuideToggle();
                renderCommands();
                hideLoading();
                isInitialized = true;
            }, 100);
        }

        // Show loading state
        function showLoading() {
            loading.classList.add('active');
            commandsContainer.innerHTML = '';
        }

        // Hide loading state
        function hideLoading() {
            loading.classList.remove('active');
        }

        // Initialize guide toggle for mobile
        function initGuideToggle() {
            // Check if mobile view
            if (window.innerWidth <= 640) {
                guideSteps.style.display = 'none';
                guideToggle.querySelector('span').textContent = '–ü–æ–∫–∞–∑–∞—Ç—å –∏–Ω—Å—Ç—Ä—É–∫—Ü–∏—é';
                isGuideExpanded = false;
            } else {
                guideSteps.style.display = 'grid';
                guideToggle.style.display = 'none';
            }
        }

        // Toggle guide visibility on mobile
        function toggleGuide() {
            isGuideExpanded = !isGuideExpanded;
            
            if (isGuideExpanded) {
                guideSteps.style.display = 'grid';
                guideSteps.classList.add('expanded');
                guideToggle.classList.add('expanded');
                guideToggle.querySelector('span').textContent = '–°–∫—Ä—ã—Ç—å –∏–Ω—Å—Ç—Ä—É–∫—Ü–∏—é';
            } else {
                guideSteps.style.display = 'none';
                guideSteps.classList.remove('expanded');
                guideToggle.classList.remove('expanded');
                guideToggle.querySelector('span').textContent = '–ü–æ–∫–∞–∑–∞—Ç—å –∏–Ω—Å—Ç—Ä—É–∫—Ü–∏—é';
            }
        }

        // Initialize Categories
        function initCategories() {
            categoriesTabs.innerHTML = '';
            
            // Add "–í—Å–µ" category first
            const allTab = createCategoryTab("–í—Å–µ", "all-tab", allCommands.length);
            categoriesTabs.appendChild(allTab);
            
            // Add other categories
            Object.entries(commandsByCategory).forEach(([category, commands]) => {
                const tab = createCategoryTab(category, "", commands.length);
                categoriesTabs.appendChild(tab);
            });
        }

        // Create category tab
        function createCategoryTab(name, extraClass = "", count = 0) {
            const tab = document.createElement('div');
            tab.className = `category-tab ${extraClass} ${name === activeCategory ? 'active' : ''}`;
            tab.textContent = name;
            tab.setAttribute('data-category', name);
            
            // Add count badge for "–í—Å–µ" category
            if (name === "–í—Å–µ") {
                const countBadge = document.createElement('span');
                countBadge.className = 'category-count';
                countBadge.textContent = count;
                tab.appendChild(countBadge);
            }
            
            tab.addEventListener('click', handleCategoryClick);
            
            return tab;
        }

        // Handle category click
        function handleCategoryClick(e) {
            const category = e.currentTarget.getAttribute('data-category');
            
            if (category === activeCategory) return;
            
            // Update tabs
            document.querySelectorAll('.category-tab').forEach(t => {
                t.classList.remove('active');
            });
            e.currentTarget.classList.add('active');
            
            // Clear search if changing category
            if (searchQuery) {
                searchBox.value = "";
                searchQuery = "";
                resultsInfo.style.display = 'none';
            }
            
            // Update category
            activeCategory = category;
            renderCommands();
            
            // Scroll to top of commands
            commandsContainer.scrollIntoView({ behavior: 'smooth', block: 'start' });
        }

        // Render Commands based on current category/search
        function renderCommands() {
            commandsContainer.innerHTML = '';
            
            let commandsToDisplay = [];
            
            if (searchQuery) {
                // Search mode
                commandsToDisplay = allCommands.filter(command => 
                    command.name.toLowerCase().includes(searchQuery.toLowerCase()) ||
                    (command.description && command.description.toLowerCase().includes(searchQuery.toLowerCase()))
                );
                
                // Show results info
                resultsInfo.style.display = 'flex';
                resultsCount.textContent = `–ù–∞–π–¥–µ–Ω–æ: ${commandsToDisplay.length} –∫–æ–º–∞–Ω–¥`;
                
                // Show/hide no results
                if (commandsToDisplay.length === 0) {
                    noResults.style.display = 'block';
                } else {
                    noResults.style.display = 'none';
                }
            } else if (activeCategory === "–í—Å–µ") {
                // All commands mode
                commandsToDisplay = allCommands;
                resultsInfo.style.display = 'none';
                noResults.style.display = 'none';
            } else {
                // Specific category mode
                commandsToDisplay = commandsByCategory[activeCategory];
                commandsToDisplay = [...commandsToDisplay].sort((a, b) => a.name.localeCompare(b.name));
                resultsInfo.style.display = 'none';
                noResults.style.display = 'none';
            }
            
            // Display commands
            commandsToDisplay.forEach((command, index) => {
                const commandCard = document.createElement('div');
                commandCard.className = 'command-card';
                commandCard.setAttribute('data-command', command.name);
                commandCard.setAttribute('role', 'button');
                commandCard.setAttribute('aria-label', `–ö–æ–º–∞–Ω–¥–∞: ${command.name}. ${command.description || '–ù–µ—Ç –æ–ø–∏—Å–∞–Ω–∏—è'}`);
                commandCard.setAttribute('tabindex', '0');
                
                const categoryLabel = activeCategory === "–í—Å–µ" || searchQuery ? 
                    `<div class="command-category">${command.category}</div>` : '';
                
                commandCard.innerHTML = `
                    <div class="command-header">
                        <div class="command-name">${command.name}</div>
                        <div class="command-cost" aria-label="–°—Ç–æ–∏–º–æ—Å—Ç—å: ${command.cost} –ë–£–°–ï–ö">
                            <i class="fas fa-coins" aria-hidden="true"></i>
                            <span>${command.cost}</span>
                        </div>
                    </div>
                    <div class="command-description">
                        ${command.description || ' '}
                        ${categoryLabel}
                    </div>
                `;
                
                // Add click/touch event
                commandCard.addEventListener('click', handleCommandClick);
                commandCard.addEventListener('keypress', (e) => {
                    if (e.key === 'Enter' || e.key === ' ') {
                        e.preventDefault();
                        handleCommandClick(e);
                    }
                });
                
                commandsContainer.appendChild(commandCard);
            });
        }

        // Handle command click
        function handleCommandClick(e) {
            const commandCard = e.currentTarget;
            const command = commandCard.getAttribute('data-command');
            
            copyToClipboard(command);
            showNotification(`"${command}" —Å–∫–æ–ø–∏—Ä–æ–≤–∞–Ω–æ!`);
            
            // Add visual feedback
            commandCard.style.transform = 'scale(0.98)';
            commandCard.style.backgroundColor = 'rgba(6, 214, 160, 0.1)';
            
            setTimeout(() => {
                commandCard.style.transform = '';
                commandCard.style.backgroundColor = '';
            }, 200);
        }

        // Handle search
        function handleSearch() {
            searchQuery = searchBox.value.trim();
            
            // If search is empty, show current category
            if (!searchQuery) {
                resultsInfo.style.display = 'none';
                renderCommands();
                return;
            }
            
            // Show all commands when searching
            if (activeCategory !== "–í—Å–µ") {
                // Update tabs to show "–í—Å–µ" as active
                document.querySelectorAll('.category-tab').forEach(t => {
                    t.classList.remove('active');
                    if (t.getAttribute('data-category') === "–í—Å–µ") {
                        t.classList.add('active');
                    }
                });
                activeCategory = "–í—Å–µ";
            }
            
            renderCommands();
        }

        // Handle clear search
        function handleClearSearch() {
            searchBox.value = "";
            searchQuery = "";
            resultsInfo.style.display = 'none';
            renderCommands();
        }

        // Copy to Clipboard with iOS fallback
        function copyToClipboard(text) {
            // iOS Safari fallback
            const isIOS = /iPad|iPhone|iPod/.test(navigator.userAgent) && !window.MSStream;
            
            if (isIOS) {
                const textArea = document.createElement('textarea');
                textArea.value = text;
                textArea.style.position = 'fixed';
                textArea.style.opacity = '0';
                document.body.appendChild(textArea);
                textArea.focus();
                textArea.select();
                
                try {
                    document.execCommand('copy');
                    console.log('–¢–µ–∫—Å—Ç —Å–∫–æ–ø–∏—Ä–æ–≤–∞–Ω (iOS fallback)');
                } catch (err) {
                    console.error('–û—à–∏–±–∫–∞ –∫–æ–ø–∏—Ä–æ–≤–∞–Ω–∏—è –Ω–∞ iOS:', err);
                }
                
                document.body.removeChild(textArea);
            } else {
                navigator.clipboard.writeText(text).then(() => {
                    console.log('–¢–µ–∫—Å—Ç —Å–∫–æ–ø–∏—Ä–æ–≤–∞–Ω');
                }).catch(err => {
                    console.error('–û—à–∏–±–∫–∞ –∫–æ–ø–∏—Ä–æ–≤–∞–Ω–∏—è:', err);
                    const textArea = document.createElement("textarea");
                    textArea.value = text;
                    document.body.appendChild(textArea);
                    textArea.select();
                    document.execCommand("copy");
                    document.body.removeChild(textArea);
                });
            }
        }

        // Show notification
        function showNotification(message) {
            notificationText.textContent = message;
            notification.classList.add('show');
            
            setTimeout(() => {
                notification.classList.remove('show');
            }, 2000);
        }

        // Handle balance button click
        function handleBalanceClick() {
            copyToClipboard('!–±—É—Å—å–∫–∏');
            showNotification('–ö–æ–º–∞–Ω–¥–∞ "!–±—É—Å—å–∫–∏" —Å–∫–æ–ø–∏—Ä–æ–≤–∞–Ω–∞ –¥–ª—è –ø—Ä–æ–≤–µ—Ä–∫–∏ –±–∞–ª–∞–Ω—Å–∞');
        }

        // Initialize on DOM load
        document.addEventListener('DOMContentLoaded', () => {
            initialize();
            
            // Event listeners
            document.getElementById('balance-value').addEventListener('click', handleBalanceClick);
            searchBox.addEventListener('input', handleSearch);
            clearSearch.addEventListener('click', handleClearSearch);
            guideToggle.addEventListener('click', toggleGuide);
            
            // Debounce search for performance
            let searchTimeout;
            searchBox.addEventListener('input', () => {
                clearTimeout(searchTimeout);
                searchTimeout = setTimeout(handleSearch, 300);
            });
            
            // Prevent iOS zoom on double tap
            document.addEventListener('touchstart', (e) => {
                if (e.touches.length > 1) {
                    e.preventDefault();
                }
            }, { passive: false });
            
            // Fix for iOS 100vh issue
            function setVH() {
                const vh = window.innerHeight * 0.01;
                document.documentElement.style.setProperty('--vh', `${vh}px`);
            }
            
            setVH();
            window.addEventListener('resize', setVH);
            window.addEventListener('orientationchange', setVH);
            
            // Handle window resize for guide toggle
            window.addEventListener('resize', () => {
                if (window.innerWidth <= 640) {
                    if (!isGuideExpanded) {
                        guideSteps.style.display = 'none';
                    }
                    guideToggle.style.display = 'flex';
                } else {
                    guideSteps.style.display = 'grid';
                    guideToggle.style.display = 'none';
                }
            });
            
            // Force iOS Safari to re-evaluate viewport
            window.addEventListener('orientationchange', () => {
                setTimeout(() => {
                    document.body.style.height = '100%';
                    document.body.style.height = 'calc(100 * var(--vh, 1vh))';
                }, 100);
            });
        });

        // Handle page visibility (for iOS resume)
        document.addEventListener('visibilitychange', () => {
            if (!document.hidden && !isInitialized) {
                initialize();
            }
        });

        // Handle page load event for iOS
        window.addEventListener('load', () => {
            document.body.style.display = 'none';
            document.body.offsetHeight;
            document.body.style.display = '';
            
            if (!isInitialized) {
                setTimeout(initialize, 100);
            }
        });

        // Fallback initialization
        if (document.readyState === 'loading') {
            document.addEventListener('DOMContentLoaded', initialize);
        } else {
            initialize();
        }
    </script>
</body>
</html>

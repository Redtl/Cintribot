    <!-- Fallback for iOS -->
    <script>
        // Check if Font Awesome loaded
        function checkFontAwesome() {
            setTimeout(function() {
                var testIcon = document.createElement('i');
                testIcon.className = 'fas fa-test';
                testIcon.style.position = 'absolute';
                testIcon.style.left = '-9999px';
                document.body.appendChild(testIcon);
                
                var isLoaded = window.getComputedStyle(testIcon).fontFamily.indexOf('Font Awesome') !== -1;
                
                if (!isLoaded) {
                    // Font Awesome not loaded, use fallback
                    document.documentElement.className += ' no-fontawesome';
                }
                
                document.body.removeChild(testIcon);
            }, 1000);
        }
        
        document.addEventListener('DOMContentLoaded', checkFontAwesome);
    </script>
    
